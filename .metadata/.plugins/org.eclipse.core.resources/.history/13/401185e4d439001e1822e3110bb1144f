package methodswithoperator3;

import java.util.Scanner;

public class bitwiseswapper {
	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		System.out.println("enter the first integer: ");
		int num1=scan.nextInt();
		System.out.println("entre the second integer: ");
		int num2=scan.nextInt();
		System.out.println("After swapping:");  
		System.out.print("first  = " +num1 +", second = " +num2);
		bitwiseswapperapp sn=new bitwiseswapperapp();
		sn.swapUsingBitwise(sn);
	}

}
